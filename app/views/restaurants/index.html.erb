<%= @restaurants.each do |restaurant| %>
  <h2><%= restaurant.name %></h2>
  <p><%= restaurant.address %></p>
  <p><%= restaurant.phone_number %></p>
  <p><%= restaurant.category %></p>

  <%= link_to "show  profile card", restaurant_path(restaurant) %>
  <%= link_to "add a new review for this restaurant", new_restaurant_review_path(restaurant) %>
<% end %>
